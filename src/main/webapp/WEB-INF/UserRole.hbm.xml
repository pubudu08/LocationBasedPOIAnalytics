<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="spring.mvc.analytics.models.UserRole" table="userRole">
        <id name="userRoleID" column="USER_ROLE_ID" type="java.lang.Integer">
        </id>
        <property name="UserRoleType" type="java.lang.String">
            <column name="USER_ROLE" not-null="true" unique="false" />
        </property>
    </class>
</hibernate-mapping>